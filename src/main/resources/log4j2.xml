<?xml version="1.0" encoding="UTF-8"?>
<Configuration name="Configuration">
    <Filter type="ThresholdFilter" level="trace"/>
    <Appenders>
        <Console name="STDOUT">
            <PatternLayout>
                <Pattern>%d{yyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n</Pattern>
            </PatternLayout>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="debug">
            <AppenderRef ref="STDOUT"/>
        </Root>
    </Loggers>
</Configuration>